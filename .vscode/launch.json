{
    "version": "0.2.0",
    "configurations": [
{
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "protocal":"inspector",
            "program": "${workspaceRoot}/server-start.js",
            "cwd": "${workspaceRoot}",
            "args": [ "${max_old_space_size:4000}" ],
            "sourceMaps": true
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to Process",
            "port": 5858
        }
    ]
}
